    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">Perilous.</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="Character">
