      <Text></Text>
    </Paragraph>
    <Paragraph Type="Dialogue">
      <Text AdornmentStyle="-1" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="">Ye</Text>
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style=""> not the Gravity of the situation...</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="General">
      <DualDialogue>
        <SpellCheckIgnoreLists>
          <IgnoredRanges/>
          <IgnoredWords/>
        </SpellCheckIgnoreLists>
        <Paragraph Type="Character">
