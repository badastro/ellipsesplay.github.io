    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="8" Style="AllCaps"></Text>
    </Paragraph>
    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">IN BLACK HOLES, DWELL WISDOM.</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">FED BY THE SPRINKLES OF STARDUST.</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">CRUSH THEIR DREAMS IN YOUR PALM.</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">IT IS CRUNCH TIME, MY BABIES.</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="Character">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">TEEN GALAXIES</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">CONSUME STAR SYSTEMS.	</Text>
      <Text></Text>
    </Paragraph>
    <Paragraph Type="Lyrics">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Final Draft" RevisionID="0" Size="12" Style="AllCaps">CONSUME WORLDS THAT TEEM.</Text>
